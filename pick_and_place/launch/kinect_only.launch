    <node name="spawn_box2" pkg="gazebo_ros" type="spawn_model" args="-file $(find pick_and_place)/urdf/objects/box.urdf -x 0.4 -z 1 -urdf -model box2" output="screen" respawn="false"/> -->

</launch>